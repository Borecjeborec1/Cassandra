    "build:dir": "electron-builder --dir",
    "buid:dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borecjeborec1/Cassandra.git"
  },
  "author": "borecjeborec1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/borecjeborec1/Cassandra/issues"
  },
  "homepage": "https://github.com/borecjeborec1/Cassandra#readme",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "16.0.8",
    "electron-reload": "^2.0.0-alpha.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "lepikjs": "^1.1.6"
  },
  "build": {
    "appId": "Cassandra",
    "win": {
      "target": "nsis",
      "icon": "./assets/icons/orchidei.png"

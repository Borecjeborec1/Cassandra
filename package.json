{
  "name": "cassandra",
  "version": "1.0.0",
  "description": "Cassandra Key Simulator",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "cassandra",
    "key",
    "simulator",
    "node",
    "electron",
    "windows"
  ],
  "scripts": {
